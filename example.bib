	Issue = {5},
	Journal = {RSC Adv.},
	Pages = {2964-2970},
	Publisher = {The Royal Society of Chemistry},
	Title = {In situ Raman spectroscopy distinguishes between reversible and irreversible thiol modifications in l-cysteine},
	Url = {http://dx.doi.org/10.1039/C6RA25879D},
	Volume = {7},
	Year = {2017},

	Keywords = {Phospholipid bilayer, Dimethylsulfoxide, Temperature dependence},
	Number = {1},
	Pages = {117 - 122},
	Title = {Temperature-dependent perturbation of phospholipid bilayers by dimethylsulfoxide},

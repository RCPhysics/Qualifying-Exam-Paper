@article{Hou2503,
	Abstract = {Label-free nonlinear optical microscopy (NLOM) based on two-photon excited fluorescence (TPEF) from cofactors nicotinamide adenine dinucleotide (NADH) and flavin adenine dinucleotide (FAD+) is widely used for high-resolution cellular redox imaging. In this work, we combined three label-free NLOM imaging methods to quantitatively characterize breast cancer cells and their relative invasive potential: (i) TPEF optical redox ratio (ORR = FAD+/NADH + FAD+), (ii) coherent Raman scattering of cellular lipids, and (iii) second harmonic generation of extracellular matrix (ECM) collagen. 3D spheroid models of primary mammary epithelial (PME) cells and breast cancer cell lines (T47D and MDA-MB-231) were characterized based on their unique ORR and lipid volume fraction signatures. Treatment with 17β-estradiol (E2) increased glycolysis in both PME and T47D ER+ breast cancer acini. However, PME cells displayed increased lipid content with no effect on ECM, while T47D cells had decreased lipid storage (P \&lt; 0.001) and significant reorganization of collagen. By measuring deuterated lipids synthesized from exogenously administered deuterium-labeled glucose, treatment of T47D cells with E2 increased both lipid synthesis and consumption rates. These results confirm that glucose is a significant source for the cellular synthesis of lipid in glycolytic breast cancer cells, and that the combination of cellular redox and lipid fraction imaging endpoints is a powerful approach with new and complementary information content.Significance: These findings provide unique insight into metabolic processes, revealing correlations between cancer metastasis and cellular redox state, lipid metabolism, and extracellular matrix. Cancer Res; 78(10); 2503{\textendash}12. {\textcopyright}2018 AACR.},
	Author = {Hou, Jue and Williams, Joshua and Botvinick, Elliot L. and Potma, Eric O. and Tromberg, Bruce J.},
	Date-Added = {2018-11-07 02:27:12 -0800},
	Date-Modified = {2018-11-07 02:27:12 -0800},
	Doi = {10.1158/0008-5472.CAN-17-2618},
	Eprint = {http://cancerres.aacrjournals.org/content/78/10/2503.full.pdf},
	Issn = {0008-5472},
	Journal = {Cancer Research},
	Number = {10},
	Pages = {2503--2512},
	Publisher = {American Association for Cancer Research},
	Title = {Visualization of Breast Cancer Metabolism Using Multimodal Nonlinear Optical Microscopy of Cellular Lipids and Redox State},
	Url = {http://cancerres.aacrjournals.org/content/78/10/2503},
	Volume = {78},
	Year = {2018},
	Bdsk-Url-1 = {http://cancerres.aacrjournals.org/content/78/10/2503},
	Bdsk-Url-2 = {https://doi.org/10.1158/0008-5472.CAN-17-2618}}
